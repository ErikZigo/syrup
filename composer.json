{
    "name": "keboola/syrup",
    "license": "MIT",
    "type": "project",
    "description": "Syrup - Concentrated extract(or)s",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
	    {
		    "type": "vcs",
		    "url": "git@bitbucket.org:keboola/google-client-bundle.git"
	    },
	    {
		    "type": "vcs",
		    "url": "git@bitbucket.org:keboola/google-analytics-bundle.git"
	    },
	    {
		    "type": "vcs",
		    "url": "git@bitbucket.org:keboola/google-drive-bundle.git"
	    }
    ],
    "require": {
        "php": ">=5.3.3",
		"symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
	    "symfony/swiftmailer-bundle": "~2.3.5",
	    "syrup/component-bundle": "~1.4.0",
	    "keboola/google-client-bundle": "1.1.*",
	    "keboola/google-analytics-bundle": "1.0.*",
	    "keboola/google-drive-bundle": "1.0.*"
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
