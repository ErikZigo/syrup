{
    "name": "keboola/syrup",
    "license": "MIT",
    "type": "project",
    "description": "Syrup - Concentrated extract(or)s",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/google-youtube-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/google-client-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/keboola-pingdom-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/syrup-slevomat-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/syrup-db-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/gooddata-writer.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/recurly-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/currency-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/google-analytics-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/google-drive-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/jsonparser-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/hierarchy-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/textsplitter-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/keboola-provisioning-bundle.git"
        },
        {
            "type": "package",
            "package": {
                "name": "gooddata/gooddata-cl",
                "version": "1.2.67",
                "dist": {
                    "url": "http://developer.gooddata.com/downloads/cl-tool/gooddata-cli.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
	    "composer/composer": "~1.0.0@alpha",
		"symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
	    "symfony/swiftmailer-bundle": "~2.3.5"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "keep-outdated": true,
            "file": "app/config/parameters.yml"
        }
    }
}
